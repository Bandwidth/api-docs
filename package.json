{
  "name": "external-api-docs",
  "version": "1.0.0",
  "description": "Handles Bandwidth's external API docs and OpenAPI specs",
  "dependencies": {
    "core-js": "^3.11.1",
    "docusaurus": "^1.14.7",
    "http-server": "^0.12.3",
    "mobx": "^6.3.0",
    "redoc": "^2.0.0-rc.53",
    "styled-components": "^5.2.3"
  },
  "scripts": {
    "postman": "cd ./postman && npm install && npm run start -- -c ./resources/postman_scaffold_collection.json -s ../external/specs/",
    "generate": "cd ./sdk-generation && npm install && npm run build && npm run generate --",
    "docsite-server": "cd ./site && npm run start",
    "generate-docsite": "cd ./site && npm run build",
    "serve": "cd ./site && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bandwidth/api-docs.git"
  },
