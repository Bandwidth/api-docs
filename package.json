  },
  "homepage": "https://github.com/Bandwidth/api-docs#readme",
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.74",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.4",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "typescript": "^4.2.4"
  }
}
