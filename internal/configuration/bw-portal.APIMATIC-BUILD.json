{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "apiSpecs": [
      "CatapultApplications",
      "CatapultUserCredentialManagement"
    ],
    "apiMergeSettings": {
      "type": "Namespaced",
      "globalApiSpecPath": "Global"
    },
    "debug": {
      "publishReport": true
    },
    "languageConfig": {
      "http": {}
    },
    "pageTitle": "Bandwidth Internal APIs - Developer Docs & API Reference",
    "navTitle": "Bandwidth Internal APIs",
    "logoUrl": "https://dev.bandwidth.com/images/mobileLogo.png",
    "logoLink": "https://www.bandwidth.com/",
    "portalSettings": {
      "enableConsoleCalls": false,
      "enableExport": false,
      "themeOverrides": {
        "palette": {
          "primaryColor": "#00bcec",
          "colors": {
            "C000": "#fff",
            "C100": "#F9FAFE",
            "C200": "#f3f3f3",
            "C300": "#e4e4e4",
            "C400": "#384A63",
            "C500": "#B0B5C9",
            "C600": "#5f5f5f",
            "C700": "#616784",
            "C800": "#333",
            "C900": "#1c0a14",
            "C1000": "#1D2236"
          }
        },
        "fontSource": [
          "https://fonts.googleapis.com/css2?family=Overpass+Mono:wght@300;400&family=Overpass:wght@100;300;400;600;900&display=swap"
        ],
        "cssStyles": {
          "headings": {
            "fontFamily": "'Overpass',sans-serif",
            "h1": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "35px",
              "fontWeight": "100",
              "fontStyle": "normal",
              "lineHeight": "0.934"
            },
            "h2": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "35px",
              "fontWeight": "100",
              "fontStyle": "normal",
              "lineHeight": "0.934"
            },
            "h3": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "21px",
              "fontWeight": "900",
              "fontStyle": "normal",
              "lineHeight": "1"
            },
            "h4": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "17.5px",
              "fontWeight": "700",
              "fontStyle": "normal",
              "lineHeight": "1.17"
            },
            "h5": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "15px",
              "fontWeight": "700",
              "fontStyle": "normal",
              "lineHeight": "1.26"
            },
            "h6": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "14px",
              "fontWeight": "600",
              "fontStyle": "normal",
              "lineHeight": "1.35"
            }
          },
          "body": {
            "fontFamily": "'Overpass',sans-serif",
            "text1": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "14px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1.7"
            },
            "text2": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "13px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1.5"
            },
            "text3": {
              "fontFamily": "'Overpass',sans-serif",
              "fontSize": "12px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1.7"
            }
          },
          "code": {
            "fontFamily": "'Overpass Mono',monospace",
            "blockCode": {
              "fontFamily": "'Overpass Mono',monospace",
              "fontSize": "14px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1.7"
            },
            "inlineCode": {
              "fontFamily": "'Overpass Mono',monospace",
              "fontSize": "14px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1.7"
            }
          }
        }
      }
    },
    "headIncludes": "<link rel=\"preconnect\" href=\"https://fonts.gstatic.com\">\n<link href=\"https://fonts.googleapis.com/css2?family=Overpass+Mono&family=Overpass:wght@100;300;400;700;900&display=swap\" rel=\"stylesheet\">\n<link href=\"static/css/styles.css\" rel=\"stylesheet\">"
  }
}
