        /realms: 
            description: |
                This resource is used to manage Realms for SIP Registrar domains.
            get:
                description: |
                    GET is used to retrieve all Realms for the account.
                queryParameters:
                    realmfragment:
                                description: The realm fragment. Min length - 3 characters.
                                type: string
                                example: myrealm
                                required: false
                responses:
                    200:
                        description: |
                            OK - the Realms retrieval request will return a list of the Realms that are created on this account.
                        body:
                            application/xml:
                                example: |
                                    <RealmsResponse>
                                        <Realms>
                                            <Realm>
                                                <Id>22</Id>
                                                <Realm>myrealm123.abc123.auth.bandwidth.com</Realm>
                                                <Default>true</Default>
                                                <Description>Test Realm 123</Description>
                                                <SipCredentialCount>3</SipCredentialCount>
                                                <Status>ACTIVE</Status>
                                            </Realm>
                                            <Realm>
                                                <Id>31</Id>
                                                <Realm>myrealm234.abc123.auth.bandwidth.com</Realm>
                                                <Default>false</Default>
                                                <Description>Test Realm 234</Description>
                                                <SipCredentialCount>6</SipCredentialCount>
                                                <Status>ACTIVE</Status>
                                            </Realm>
                                        </Realms>
                                    </RealmsResponse>
                    400:
                        description: |
                            Bad Request - realmfragment should contain at least 3 characters.
                        body:
                            application/xml:
                                example: |
                                    <RealmsResponse>
                                        <ResponseStatus>
                                            <ErrorCode>33011</ErrorCode>
                                            <Description>realmfragment should contain at least 3 characters.</Description>
                                        </ResponseStatus>
                                    </RealmsResponse>
                    404:
                        description: |
                            Not Found - if the account specified does not exist or is locked this will result in a not found.
                        body:
                            application/xml:
                                example: |
                                    <RealmsResponse>
                                        <ResponseStatus>
                                            <ErrorCode>4022</ErrorCode>
                                            <Description>Account '2' does not exist or is locked</Description>
                                        </ResponseStatus>
                                    </RealmsResponse>
            post:
                description: |
                    POST is used to create a Realm.<br>
                    The key data elements in the submission are:
                     <table style="text-align: left; width: 80%;"
                     border="1" cellpadding="2" cellspacing="2">
                      <tbody>
                        <tr>
                          <td>Realm</td>
                          <td>Realm name. Field is required. Max length - 64 characters.</td>
                        </tr>
                        <tr>
                          <td>Default</td>
                          <td>Default Realm flag. Field is required. [true|false]</td>
                        </tr>
                        <tr>
                          <td>Description</td>
                          <td>Realm Description. Field is optional. Max length - 255 characters.</td>
                        </tr>
                      </tbody>
                    </table>
                body:
                    application/xml:
                        example: |
                            <Realm>
                                <Realm>realm123</Realm>
                                <Description>This is realm #123</Description>
                                <Default>true</Default>
                            </Realm>
                responses:
                    201:
                        description: |
                            OK - the Realm was successfully created.
                        body:
                            application/xml:
                                example: |
                                    <RealmResponse>
                                        <Realm>
                                            <Id>22</Id>
                                            <Realm>realm123.abc123.auth.bandwidth.com</Realm>
                                            <Description>This is realm #123</Description>
                                            <Default>true</Default>
                                            <SipCredentialCount>0</SipCredentialCount>
                                            <Status>CREATE_PENDING</Status>
                                        </Realm>
                                    </RealmResponse>
                    400:
                        description: |
                            Bad Request - the Realm was not created.
                        body:
                            application/xml:
                                example: |
                                    <RealmResponse>
                                        <ResponseStatus>
                                            <ErrorCode>33003</ErrorCode>
                                            <Description>Realm 'realmmmmmmmmmmmmmmmmmmmmmmmmmmm' is too long (30 characters max)</Description>
                                        </ResponseStatus>
                                    </RealmResponse>
            /{realm}: 
                description: |
                    This resource is used to manage a single Realm for SIP Registrar.<br>
                    There are three components to the Realms request payload:
                    <ol>
                    <li>Realm (required) - A string identifying the Realm name.</li>
                    <li>Default (required) - A flag indicating whether the Realm is the default for the account. [true|false]</li>
                    <li>Description (optional) - A string describing the Realm.</li>
                    </ol>
                    Three additional informational fields will be returned as part of the RealmResponse:
                    <ol>
                    <li>Id - The ID number associated with the Realm.</li>
                    <li>SipCredentialCount - The number of Sip Credentials associated with the Realm.</li>
                    <li>Status - Indicates the status of the Realm. Only 'active' Realms can be used to associate with Sip Credentials. Possible statuses are: [ACTIVE|CREATE_PENDING|CREATE_FAILED|DELETE_PENDING|DELETE_FAILED]</li>
                    </ol>
                get:
                    description: |
                        GET is used to retrieve a Realm associated with the account.
                    responses:
                        200:
                            description: |
                                OK - will return full information about a Realm on this account.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <Realm>
                                                <Id>22</Id>
                                                <Realm>realm123.abc123.auth.bandwidth.com</Realm>
                                                <Description>This is realm #123</Description>
                                                <Default>false</Default>
                                                <SipCredentialCount>15</SipCredentialCount>
                                                <Status>ACTIVE</Status>
                                            </Realm>
                                        </RealmResponse>
                        404:
                            description: |
                                Not Found - if the account specified does not exist, is locked or there is no Realm, it will result in a not found.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <ResponseStatus>
                                                <Description>The resource does not exist</Description>
                                            </ResponseStatus>
                                        </RealmResponse>
                put:
                    description: |
                        PUT is used to update a Realm.<br>
                        The key data elements in the submission are:
                         <table style="text-align: left; width: 80%;"
                         border="1" cellpadding="2" cellspacing="2">
                          <tbody>
                            <tr>
                              <td>Default</td>
                              <td>Default Realm flag. A Realm can only be updated to become the new default, ie: Default cannot be set to 'false'. Field is optional. </td>
                            </tr>
                            <tr>
                              <td>Description</td>
                              <td>Realm Description. Field is optional. Max length - 255 characters.</td>
                            </tr>
                          </tbody>
                        </table>
                    body:
                        application/xml:
                            example: |
                                <Realm>
                                    <Default>true</Default>
                                    <Description>This is realm has been updated.</Description>
                                </Realm>
                    responses:
                        200:
                            description: |
                                OK - the Realm was successfully updated.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <Realm>
                                                <Id>22</Id>
                                                <Realm>realm123.abc123.auth.bandwidth.com</Realm>
                                                <Default>true</Default>
                                                <Description>This is realm has been updated.</Description>
                                                <SipCredentialCount>0</SipCredentialCount>
                                                <Status>ACTIVE</Status>
                                            </Realm>
                                        </RealmResponse>
                        400:
                            description: |
                                Bad Request - cannot update Realm value.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <ResponseStatus>
                                                <ErrorCode>33005</ErrorCode>
                                                <Description>Cannot update Realm value. Instead, you must delete and recreate it.</Description>
                                            </ResponseStatus>
                                        </RealmResponse>
                delete:
                    description: |
                        DELETE is used to delete Realm.
                    responses:
                        202:
                            description: |
                                Accepted - the Realm is pending deletion.
                        400:
                            description: |
                                Bad Request - the Realm could not be deleted. Cannot delete default realm.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <ResponseStatus>
                                                <ErrorCode>33006</ErrorCode>
                                                <Description>Cannot delete default Realm. Please assign a new Realm to be the default.</Description>
                                            </ResponseStatus>
                                        </RealmResponse>
                        404:
                            description: |
                                Not Found - the Realm was not found on the account.
                            body:
                                application/xml:
                                    example: |
                                        <RealmResponse>
                                            <ResponseStatus>
                                                <ErrorCode>33001</ErrorCode>
                                                <Description>Realm 'realmxyz' was not found</Description>
                                            </ResponseStatus>
                                        </RealmResponse>
