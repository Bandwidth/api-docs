                description: Retrieves the LCAs associated with that telephone number.
                responses:
                    200:
                        description: The LCAs have been successfully retrieved and displayed.
                        body:
                            application/xml:
                                example:
                                    <SearchResult>
                                        <ListofNPANXX>
