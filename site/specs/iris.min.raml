                                                    <!-- CallbackPassword is omitted for security -->
                                                </Credentials>
                                            </Callback>
                                        </EmergencyNotificationRecipient>
                                    </EmergencyNotificationRecipientsResponse>
                    400:
                        description: Bad request <br>
                            <table border="1" cellspacing="0" cellpadding="0" width="624">
                                <tbody>
                                    <tr>
                                        <td valign="top">
                                            <p>
                                                <strong>Condition</strong>
                                            </p>
                                        </td>
                                        <td width="351" valign="top">
                                            <p>
                                                <strong>Error Description</strong>
                                            </p>
                                        </td>
